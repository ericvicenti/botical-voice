{
  "name": "Botical3",
  "version": "1.0.0",
  "description": "Real-time voice assistant with LiveKit Agents",
  "type": "module",
  "scripts": {
    "dev": "bun run build:client && trap 'kill 0' EXIT; bun run src/token-server.ts & bun run src/main.ts dev",
    "dev:agent": "bun run src/main.ts dev",
    "build:client": "bun build client/index.html --outdir dist/client --minify --sourcemap=external",
    "build:livekit": "cd livekit-server && go build -o ../dist/livekit-server ./cmd/server",
    "start": "bun run src/main.ts start",
    "download-files": "bun run src/main.ts download-files",
    "client": "bun run src/token-server.ts"
  },
  "license": "ISC",
  "patchedDependencies": {
    "@livekit/agents": "patches/@livekit__agents.patch",
    "@livekit/agents-plugin-cartesia": "patches/@livekit__agents-plugin-cartesia.patch"
  },
  "dependencies": {
    "@livekit/agents": "^1.0.47",
    "@livekit/agents-plugin-cartesia": "^1.0.47",
    "@livekit/agents-plugin-deepgram": "^1.0.47",
    "@livekit/agents-plugin-livekit": "^1.0.47",
    "@livekit/agents-plugin-openai": "^1.0.47",
    "@livekit/agents-plugin-silero": "^1.0.47",
    "livekit-client": "^2.0.0",
    "livekit-server-sdk": "^2.15.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
